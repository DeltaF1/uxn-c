( hello world )

%RTN { JMP2r }

( devices )

|0100 ;System { vector 2 pad 6 r 2 g 2 b 2 }
|0110 ;Console { pad 8 char 1 byte 1 short 2 }

( program )

|0200 
	
	( for loop )
	#00 #0d
	$loop
		( body ) 
		SWP #01 ADD SWP
		DUP2 LTH ^$loop JNZ
	POP2

	( while )
	#00 #0d
	$while
		( body )
		DUP2 EQU ^$end JNZ
		SWP #01 ADD SWP
	^$while JMP $end
	POP2

	( switch )
	#02 
	DUP #01 NEQ ^$b JNZ
		( a ) 
	$b DUP #02 NEQ ^$c JNZ
		( b ) 
	$c DUP #03 NEQ ^$default JNZ
		( c ) 
	$default
	POP

BRK
